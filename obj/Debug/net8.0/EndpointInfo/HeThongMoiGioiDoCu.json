{
  "openapi": "3.0.1",
  "info": {
    "title": "HeThongMoiGioiDoCu",
    "version": "1.0"
  },
  "paths": {
    "/api/account/signup": {
      "post": {
        "tags": [
          "Account"
        ],
        "requestBody": {
          "content": {
            "multipart/form-data": {
              "schema": {
                "type": "object",
                "properties": {
                  "Username": {
                    "type": "string"
                  },
                  "Password": {
                    "type": "string"
                  },
                  "Email": {
                    "type": "string"
                  },
                  "Fullname": {
                    "type": "string"
                  },
                  "Gender": {
                    "type": "boolean"
                  },
                  "DateOfBirth": {
                    "type": "string",
                    "format": "date-time"
                  },
                  "Address": {
                    "type": "string"
                  },
                  "PhoneNumber": {
                    "type": "string"
                  }
                }
              },
              "encoding": {
                "Username": {
                  "style": "form"
                },
                "Password": {
                  "style": "form"
                },
                "Email": {
                  "style": "form"
                },
                "Fullname": {
                  "style": "form"
                },
                "Gender": {
                  "style": "form"
                },
                "DateOfBirth": {
                  "style": "form"
                },
                "Address": {
                  "style": "form"
                },
                "PhoneNumber": {
                  "style": "form"
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/account/signin": {
      "post": {
        "tags": [
          "Account"
        ],
        "requestBody": {
          "content": {
            "multipart/form-data": {
              "schema": {
                "type": "object",
                "properties": {
                  "Email": {
                    "type": "string"
                  },
                  "Password": {
                    "type": "string"
                  }
                }
              },
              "encoding": {
                "Email": {
                  "style": "form"
                },
                "Password": {
                  "style": "form"
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/account/logout": {
      "post": {
        "tags": [
          "Account"
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/admin/account/signin": {
      "post": {
        "tags": [
          "Account"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/SigninDto"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/SigninDto"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/SigninDto"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/admin/account/logout": {
      "post": {
        "tags": [
          "Account"
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "SigninDto": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "nullable": true
          },
          "password": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      }
    }
  }
}